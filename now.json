{
  "name": "janet-bot",
  "alias": "janet-habit-bot",
  "version": 2.0,
  "builds": [{"src": "index.js", "use": "@now/node-server"}],
  "routes": [
    { "src": "/.*", "dest": "index.js" }
  ],
  "env": {
    "BOT_TOKEN": "@telegram-bot-api",
    "MONGO_USER": "@mongo-db-username",
    "MONGO_PASSWORD": "@mongo-db-password"
  }

}